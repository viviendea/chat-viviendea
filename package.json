{
  "name": "chat-viviendea",
  "private": false,
  "version": "1.0.4",
  "type": "module",
  "description": "Widget de chat interactivo para sitios web de Viviendea",
  "main": "widget-dist/chat-widget.umd.js",
  "module": "widget-dist/chat-widget.es.js",
  "types": "widget-dist/widget.d.ts",
  "files": [
    "widget-dist/"
  ],
  "jsdelivr": "widget-dist/chat-widget.umd.js",
  "unpkg": "widget-dist/chat-widget.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/viviendea/chat-viviendea.git"
  },
  "keywords": [
    "chat",
    "widget",
    "viviendea",
    "customer-support",
    "react"
  ],
  "author": "Viviendea",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:widget": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "build:analyze": "npm run build && echo 'Build completed for analysis'"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
